<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cim.itmk.web.user_menu.mapper.UserMenuMapper">
    <insert id="saveMenu" parameterType="java.util.Map">
        INSERT INTO user_menu (user_id, menu_id) VALUES
        <foreach collection="menuIds" item="menuId" separator="," open="(" close=")">
            #{userId}, #{menuId}
        </foreach>
    </insert>
</mapper>
